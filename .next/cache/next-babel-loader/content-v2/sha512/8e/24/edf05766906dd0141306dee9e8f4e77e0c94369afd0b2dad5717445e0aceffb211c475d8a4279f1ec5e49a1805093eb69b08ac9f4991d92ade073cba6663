   }}).then().then(r => {\r\n                            router.push('/business');\r\n                            authCtx.login(tRef.current.value,accArr.firstName, accArr.lastName, isBusiness);\r\n                            })}\r\n    }\r\n        else {\r\n              return res.json().then(data => {\r\n                let errorMessage = 'Auth failed';\r\n                if (data && data.error && data.error.message){ \r\n                  errorMessage = data.error.message;\r\n                }\r\n                alert(errorMessage);\r\n              })}})\r\n        };\r\n    const checkValid = (e) => {\r\n        if (e.target.name === 'business' || e.target.name  === 'first') {\r\n            console.log('hh')\r\n            if (e.target.value.length > 1){\r\n                \r\n                setFName(true);\r\n                return\r\n            }\r\n            setFName(false);\r\n\r\n        }\r\n        else if ( e.target.name === 'address' || e.target.name === 'last'){\r\n            if (e.target.value.length > 7) {\r\n                setSName(true);\r\n                return \r\n            }\r\n            setSName(false);\r\n\r\n        }\r\n        else if (e.target.name === 'email') {\r\n            if (e.target.value.length !=0 && (e.target.value.includes('.com') || e.target.value.includes('.net') || e.target.value.includes('.edu')) && e.target.value.includes('@')){\r\n                console.log('this works');\r\n                setTName(true);\r\n                return;\r\n            }\r\n            setSName(false);\r\n        }\r\n        else {\r\n            if (e.target.value.length > 6 && /\\d/.test(e.target.value) && checkUpper(e.target.value)){\r\n                console.log('this works');\r\n                setFoName(true);\r\n                return;\r\n            }\r\n            console.log('improper password');\r\n            setFoName(false);\r\n            \r\n        }\r\n        \r\n    } \r\n    const setBack = (e) => {\r\n        if (e.target.name === 'business' || e.target.name  === 'first') {\r\n           setFName(null);\r\n\r\n        }\r\n        else if ( e.target.name === 'address' || e.target.name === 'last'){\r\n           setSName(null);\r\n\r\n        }\r\n        else if (e.target.name === 'email') {\r\n            setTName(null);\r\n        }\r\n        else {\r\n        setFoName(null);\r\n        \r\n    } \r\n    }      \r\n    if (isBusiness){\r\n        return  <Card> <h1>Signup Business</h1>\r\n       <div className={styles.buttonBusiness}> <button onClick = {businessHandler}>&#x2190;</button><span>&#9678;</span>&#9673;<span></span><button>&#x2192;</button></div> \r\n        <div className= {styles.inputContainer}>\r\n        <label htmlFor=\"business\">Business Name</label>\r\n        <input onInput = {setBack} onBlur = {checkValid} ref = {fRef} name = 'business'/>\r\n        <label htmlFor=\"address\">Address</label>\r\n        <input onInput = {setBack} ref = {sRef} type = \"text\" name = 'address'/>\r\n        <label htmlFor=\"email\">Email</label>\r\n        <input onInput = {setBack} onBlur = {checkValid}  ref = {tRef} type = \"email\" name = 'email'/>\r\n        <label htmlFor=\"password\">Password</label>\r\n        <input onInput = {setBack} onBlur = {checkValid} ref = {foRef} type = \"password\" name = \"password\"/>\r\n        </div>\r\n        <button onClick = {signup} className = {styles.buttonSignUp}>Sign Up</button>\r\n        </Card>\r\n    }\r\n    return <Card> <h1>Signup Customer</h1>\r\n           <div className={styles.buttonBusiness}> <button>&#x2190;</button><span>&#9673;</span>&#9678;<span></span><button onClick={businessHandler}>&#x2192;</button></div> \r\n    <div className= {styles.inputContainer}>\r\n    <label htmlFor=\"first\">First Name</label>\r\n    <input onInput = {setBack} onBlur = {checkValid} type = \"text\" ref = {fRef} name = 'first' className={`${fName ? styles.flagged : ''}`}/>\r\n    <label htmlFor=\"last\">Last Name</labe